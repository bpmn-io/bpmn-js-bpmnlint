{
  "name": "bpmn-js-bpmnlint",
  "version": "0.10.1",
  "description": "bpmn-js integration for bpmnlint",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "source": "lib/index.js",
  "scripts": {
    "all": "run-s test distro copy-assets",
    "test": "karma start --no-auto-test --single-run",
    "dev": "karma start",
    "distro": "run-s bundle test:bundle",
    "bundle": "NODE_ENV=production rollup -c",
    "test:bundle": "echo \"NO TESTS YET\"",
    "copy-assets": "cpx 'assets/css/*' dist/assets/css -v",
    "prepublishOnly": "run-s bundle copy-assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-js-bpmnlint.git"
  },
  "author": "Philipp Fromme <https://github.com/philippfromme>",
  "contributors": [
    {
      "name": "Nico Rehwaldt",
      "url": "https://github.com/nikku"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "bpmn-js": "^2.4.1",
    "bpmnlint": "^5.0.0",
    "bpmnlint-loader": "^0.1.3",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.19.0",
    "raw-loader": "^1.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-string": "^3.0.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.37.0"
  },
  "dependencies": {
    "min-dash": "^3.1.0",
    "min-dom": "^3.1.1"
  },
  "peerDependencies": {
    "bpmnlint": "^3.2.0 || ^4.0.0 || ^5.0.0"
  }
}
